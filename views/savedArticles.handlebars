{{#each dbArticle}}
<div data-_id={{_id}} class="card">
    <div class="card-header">
        <h3>
            <a class="article-link" target="_blank" rel="noopener noreferrer" href={{url}}>{{headline}}</a>

        </h3>
        <button class="btn btn-danger deleteBtn">Delete From Saved</button>
        <button class="btn btn-success notesBtn">Article Notes</button>
    </div>
    <div class="card-body">{{summary}}</div>
</div>
{{/each}}

<style type="text/css">
    .scrapeBtn {
        display: none;
    }
</style>